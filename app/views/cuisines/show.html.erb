<div class="container">
  <h1 class="mt-5 mb-4">Cozinha</h1>

  <table class="table">
    <thead>
      <tr>
        <th scope="col">Nome</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @cuisine.name %></td>
      </tr>
    </tbody>
  </table>

  <div class="col-sm-5">
    <%= link_to 'Editar', edit_cuisine_path(@cuisine), class:"btn btn-warning" %>
    <%= link_to 'Voltar', cuisines_path, class:"btn btn-outline-dark" %>
  </div>
</div>